import java.util.Random;

public class Input {

	static double Z = 9999.0;
	static double B = 15.0; // Budget: number of hours defender has.

	// Region 1 (SGC Paper)
	// static int targets = 7;
	// static double[] defCov = { 11.0, 7.0, 14.0, 15.0, 19.0, 16.0, 17.0 };
	// static double[] defUCov = { -22.0, -14.0, -35.0, -19.0, -38.0, -14.0,
	// -21.0 };
	// static double[] attCov = { -21.0, -31.0, -13.0, -16.0, -25.0, -33.0,
	// -41.0 };
	// static double[] attUCov = { 14.0, 23.0, 30.0, 17.0, 76.0, 16.0, 64.0 };
	// static double[] hours = { 9.0, 3.0, 4.0, 8.0, 7.0, 6.0, 5.0 };

	// Region 2
	static int targets = 6;
	static double[] defCov = { 12.0, 20.0, 17.0, 16.0, 15.0, 18.0 };
	static double[] defUCov = { -30.0, -3.0, -12.0, -18.0, -17.0, -4.0 };
	static double[] attCov = { -14.0, -30.0, -15.0, -17.0, -20.0, -13.0 };
	static double[] attUCov = { 19.0, 30.0, 8.0, 9.0, 23.0, 14.0 };
	static double[] hours = { 5.0, 12.0, 7.0, 15.0, 7.0, 9.0 };

	// Region 3
	// static int targets = 7;
	// static double[] defCov = { 14.0, 15.0, 14.0, 9.0, 14.0, 14.0, 19.0 };
	// static double[] defUCov = { -20.0, -30.0, -24.0, -24.0, -9.0, -19.0,
	// -10.0 };
	// static double[] attCov = { -26.0, -10.0, -22.0, -27.0, -16.0, -14.0,
	// -21.0 };
	// static double[] attUCov = { 2.0, 10.0, 9.0, 6.0, 11.0, 18.0, 13.0 };
	// static double[] hours = { 9.0, 14.0, 6.0, 9.0, 11.0, 14.0, 12.0 };

	public static int randomNumberInRange(int min, int max) {
		Random random = new Random();
		return random.nextInt((max - min) + 1) + min;
	}

	// When input called.
	public static void input() {
		for (int t = 0; t < targets; t++) {
			// defCov[t] = randomNumberInRange(0, 100);
			attUCov[t] = randomNumberInRange(1, 20);
			defUCov[t] = randomNumberInRange(-30, -1);
			attCov[t] = randomNumberInRange(-30, -1);
			// hours[t] = randomNumberInRange(5, 15);
		}

	}

}
